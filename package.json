{
  "name": "EPRI Backend",
  "version": "1.0.0",
  "description": "Backend API for EPRI - Express.js with Prisma ORM",
  "main": "dist/server-auth.js",
  "scripts": {
    "dev": "nodemon src/server-auth.ts",
    "dev:clean": "nodemon src/server-clean.ts",
    "dev:simple": "nodemon src/server-minimal.ts",
    "dev:api": "nodemon src/server.ts",
    "build": "tsc",
    "start": "node dist/server-auth.js",
    "prisma:generate": "npx prisma generate",
    "prisma:migrate": "npx prisma migrate dev",
    "prisma:studio": "npx prisma studio",
    "prisma:seed": "ts-node prisma/seed.ts",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
  },
  "keywords": [
    "EPRI",
    "ecommerce",
    "express",
    "prisma",
    "typescript",
    "rest-api"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^6.19.0",
    "@reduxjs/toolkit": "^2.9.2",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "express-validator": "^7.2.1",
    "i18next": "^25.7.1",
    "i18next-fs-backend": "^2.6.1",
    "i18next-http-middleware": "^3.8.2",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "pg": "^8.16.3",
    "react-redux": "^9.2.0",
    "stripe": "^17.5.0",
    "swiper": "^12.0.3"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.10.5",
    "@typescript-eslint/eslint-plugin": "^8.20.0",
    "@typescript-eslint/parser": "^8.20.0",
    "eslint": "^9.18.0",
    "nodemon": "^3.1.9",
    "prettier": "^3.4.2",
    "prisma": "^6.19.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2"
  }
}
